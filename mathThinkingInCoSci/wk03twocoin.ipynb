    "def change(amount):\n",
    "    assert(amount >= 8)\n",
    "    if amount == 8:\n",
    "      return [3, 5]\n",
    "    if amount == 9:\n",
    "      return [3, 3, 3]\n",
    "    if amount == 10:\n",
    "      return [5, 5]\n",
    "\n",
    "    coins = change(amount - 3)\n",
    "    coins.append(3)\n",
    "    return coins\n",
    "\n",
    "print(change(97))"
   ]
  }
